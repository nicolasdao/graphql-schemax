{
  "name": "graphql-schemax",
  "version": "0.0.12",
  "description": "Creates GraphQL string schema from plain JSON objects.",
  "keywords": [
    "graphql",
    "schema"
  ],
  "main": "./dist/index.cjs",
  "exports": {
    ".": [
      {
        "import": "./src/index.mjs",
        "require": "./dist/index.cjs",
        "default": "./dist/index.cjs"
      },
      "./dist/index.cjs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolasdao/graphql-schemax.git"
  },
  "bugs": {
    "url": "https://github.com/nicolasdao/graphql-schemax/issues"
  },
  "homepage": "https://github.com/nicolasdao/graphql-schemax#readme",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src/ test/ --fix --ext .mjs",
    "push": "npm run build && git push --follow-tags origin master && npm publish",
    "rls": "standard-version --release-as",
    "test": "mocha --exit",
    "v": "node -e \"console.log(require('./package.json').version)\""
  },
  "author": "Nicolas Dao",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "mocha": "^9.1.2",
    "rollup": "^2.58.0",
    "rollup-plugin-multi-input": "^1.3.1",
    "standard-version": "^9.3.1"
  }
}
